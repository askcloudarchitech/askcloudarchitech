{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": true,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "videoID",
          "name": "videoID",
          "type": "string",
          "single": true
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "list"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "ID",
          "name": "ID",
          "type": "datetime"
        },
        {
          "title": "Main Image",
          "name": "previewImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "OLD main image",
          "name": "mainImage",
          "type": "string",
          "single": true
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "[[workspace]]/asksitestatic/static",
  "frontMatter.content.hideFm": false,
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/asksitestatic/content/posts/digital-marketing",
      "title": "digital-marketing",
      "previewPath": "/posts/digital-marketing/"
    },
    {
      "path": "[[workspace]]/asksitestatic/content/posts/tech-terms",
      "title": "tech-terms",
      "previewPath": "/posts/tech-terms/"
    },
    {
      "title": "working-at-home",
      "path": "[[workspace]]/asksitestatic/content/posts/working-at-home",
      "previewPath": "/posts/working-at-home/"
    },
    {
      "title": "concepts",
      "path": "[[workspace]]/asksitestatic/content/posts/concepts",
      "previewPath": "/posts/concepts/"
    },
    {
      "title": "self-improvement",
      "path": "[[workspace]]/asksitestatic/content/posts/self-improvement",
      "previewPath": "/posts/self-improvement/"
    },
    {
      "title": "tools",
      "path": "[[workspace]]/asksitestatic/content/posts/tools",
      "previewPath": "/posts/tools/"
    },
    {
      "title": "tutorials",
      "path": "[[workspace]]/asksitestatic/content/posts/tutorials",
      "previewPath": "/posts/tutorials/"
    },
    {
      "title": "career-advice",
      "path": "[[workspace]]/asksitestatic/content/posts/career-advice",
      "previewPath": "/posts/career-advice/"
    }
  ],
  "frontMatter.content.snippets": {
    "CustomImage": {
      "description": "insert an image",
      "body": "{{< img2 src=\"[[&mediaUrl]]\" alt=\"[[alt]]\" >}}",
      "isMediaSnippet": true
    },
    "Youtube video": {
      "description": "youtube video",
      "body": "{{< youtube [[videoID]] >}}",
      "fields": [
        {
          "title": "videoID",
          "name": "videoID",
          "type": "string",
          "single": true
        }
      ]
    }
  }
}